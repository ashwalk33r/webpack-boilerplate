		"prettier": "prettier --check \"**/*.{ts,js,json,html,css,sass,md}\"",
		"prettier:fix": "prettier --write \"**/*.{ts,js,json,html,css,sass,md}\""
	},
	"dependencies": {
		"lodash": "^4.17.21",
		"p5": "^1.3.1",
