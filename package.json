		"sass": "^1.32.12",
		"sass-loader": "^11.0.1",
		"style-loader": "^2.0.0",
		"ts-loader": "^9.1.1",
		"ts-node": "^9.1.1",
		"tsconfig-paths": "^3.9.0",
		"typescript": "^4.2.4",
		"webpack": "^5.36.2",
		"webpack-cli": "^4.6.0",
		"webpack-dev-server": "^3.11.2"
