		"@types/mini-css-extract-plugin": "^1.4.2",
		"@types/node": "^15.0.1",
		"@types/p5": "^0.9.1",
		"@types/webpack": "^5.28.0",
		"@typescript-eslint/eslint-plugin": "^4.22.0",
		"@typescript-eslint/parser": "^4.22.0",
		"cross-env": "^7.0.3",
